{
    "name": "wc3maptranslator",
    "version": "4.0.1",
    "description": "Module to translate between `war3map` and `json` formats for WarCraft III .w3x maps",
    "keywords": [
        "wc3",
        "warcraft",
        "reforged",
        "frozen throne",
        "reign of chaos",
        "war3map",
        "translator",
        "json",
        "w3x"
    ],
    "main": "index.js",
    "scripts": {
        "build": "tsc --build",
        "test": "npm run build && mocha",
        "test-c8": "npm run build && c8 mocha",
        "test-travis": "npm run build && ./node_modules/istanbul/lib/cli.js cover ./node_modules/c8/bin/c8 --include-all-sources true --root ./lib -- -R spec ./test/*.js"
    },
    "engines": {
        "node": ">=14",
        "npm": ">=7",
        "tsc": ">3"
    },
    "repository": {
        "type": "git",
        "url": "git+https://chiefofgxbxl@github.com/ChiefOfGxBxL/WC3MapTranslator.git"
    },
    "author": "ChiefOfGxBxL",
    "contributors": [
        "ChiefOfGxBxL",
        "dulingzhi",
        "rufreakde",
        "ttay24",
        "yatyricky"
    ],
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/ChiefOfGxBxL/WC3MapTranslator/issues"
    },
    "homepage": "https://github.com/ChiefOfGxBxL/WC3MapTranslator#readme",
    "dependencies": {
        "intn": "^1.0.0",
        "round-to": "^5.0.0",
        "ieee754": "^1.2.1"
    },
    "devDependencies": {
        "@istanbuljs/nyc-config-typescript": "^1.0.2",
        "@types/fs-extra": "^11.0.1",
        "@types/mocha": "^10.0.1",
        "@types/node": "^20.4.4",
        "@types/round-to": "^4.0.0",
        "@typescript-eslint/eslint-plugin": "^6.1.0",
        "c8": "^8.0.0",
        "coveralls": "^3.1.1",
        "diff-buf": "^1.0.1",
        "eslint": "^8.45.0",
        "eslint-config-standard-with-typescript": "^37.0.0",
        "eslint-plugin-import": "^2.27.5",
        "eslint-plugin-n": "^16.0.1",
        "eslint-plugin-promise": "^6.1.1",
        "fs-extra": "^11.1.1",
        "mocha": "^10.2.0",
        "source-map-support": "^0.5.21",
        "ts-node": "^10.9.1",
        "typescript": "^5.1.6"
    }
}
